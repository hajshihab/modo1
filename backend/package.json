{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "E-commerce platform backend API with Firebase Functions and Supabase",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "firebase:serve": "firebase emulators:start",
    "firebase:deploy": "firebase deploy",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:generate-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "db:migrate": "node dist/scripts/migrate.js",
    "db:seed": "node dist/scripts/seed.js"
  },
  "dependencies": {
    "express": "^4.18.2",
