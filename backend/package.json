    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "cron": "^3.1.6",
    "redis": "^4.6.11",
    "bull": "^4.12.2",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "passport": "^0.7.0",
